{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "jsx": "react-native",
        "strict": true,
        "resolveJsonModule": true,
        "baseUrl": "./",
        "paths": {
            "@src/*": ["src/*/index", "src/*"]
        }
    },
    "exclude": ["node_modules", "./node_modules", "./node_modules/*", "./node_modules/@types/node/index.d.ts"],
    "typeRoots": ["./src/types"]
}
